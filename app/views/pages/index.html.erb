<div id="app">
  <h2>Reviews</h2>
  <ul>
  <% @reviews.each do |review| %>
    <li><%= review %></li>
  <% end %>
  <div v-for="review in reviews" v-if="isPositive(review.text)">
    <h3> {{ review.text }}</h3>
    <h3> {{ review.rating }}</h3>
    <h3> {{ review.reviewer }}</h3>
  </div>
<div>
  <input type="text" v-model="newReviewText">
  <input type="text" v-model="newReviewRating">
  <input type="text" v-model="newReviewReviewer">
  <button onclick="addReview()">Add Review</button>
</div>
</div>
